{
  "name": "easy-confetti",
  "version": "0.0.2",
  "description": "EasyConfetti is designed to create confetti animations and provides flexible, customisable confetti animations that can be easily integrated into a web page to create visual effects.",
  "author": "ux-ui.pro",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/ux-ui-pro/easy-confetti.git"
  },
  "bugs": {
    "url": "https://github.com/ux-ui-pro/easy-confetti/issues"
  },
  "homepage": "https://github.com/ux-ui-pro/easy-confetti",
  "sideEffects": false,
  "browserslist": "> 0.5%, last 2 versions, not dead",
  "scripts": {
    "clean": "rm -rf dist .parcel-cache",
    "build": "yarn clean && parcel build",
    "lint:js": "eslint **/*.{ts,js}",
    "lintfix": "yarn lint:js --fix"
  },
  "main": "dist/index.js",
  "module": "dist/index.module.js",
  "types": "dist/index.d.ts",
  "targets": {
    "main": {
      "source": "src/EasyConfetti.ts"
    },
    "module": {
      "source": "src/EasyConfetti.ts"
    },
    "types": {
      "source": "src/EasyConfetti.ts"
    }
  },
  "devDependencies": {
    "@parcel/packager-ts": "2.12.0",
    "@parcel/transformer-sass": "2.12.0",
    "@parcel/transformer-typescript-types": "2.12.0",
    "@typescript-eslint/eslint-plugin": "^7.11.0",
    "@typescript-eslint/parser": "^7.11.0",
    "eslint": "^7.32.0 || ^8.57.0",
    "eslint-config-airbnb-base": "^15.0.0",
    "eslint-plugin-import": "^2.29.1",
    "parcel": "^2.12.0",
    "typescript": "^5.4.5"
  },
  "keywords": [
    "javascript",
    "confetti",
    "animation",
    "canvas",
    "particle effects",
    "web development",
    "front-end",
    "DOM manipulation",
    "customizable effects",
    "web animations",
    "easy confetti"
  ]
}
